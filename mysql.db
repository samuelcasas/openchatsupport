create table CHAT(
	ID VARCHAR(10) not NULL UNIQUE PRIMARY KEY,
	ID_CHAT VARCHAR(10) not NULL,
	ID_SOPORTE VARCHAR(10) not NULL,
	ID_CLIENTE VARCHAR(10) not NULL,
	SESSION TEXT not NULL,
	MENSAJE TEXT not NULL,
	FECHA TEXT not NULL, 
	SENDER VARCHAR(10) not NULL
	);

create table CHAT_GESTION(
	ID VARCHAR(10) not NULL UNIQUE PRIMARY KEY,
	ID_SOPORTE VARCHAR(10) not NULL,
	ID_CLIENTE VARCHAR(10) not NULL,
	SESSION TEXT not NULL,
	NAVEGADOR TEXT not NULL,
	IP VARCHAR(100) not NULL,
	UBICACION VARCHAR(100) not NULL,
	NOMBRE VARCHAR(100) not NULL,
	EMAIL VARCHAR(100) not NULL,
	TELEFONO VARCHAR(100) not NULL,
	FECHA TEXT not NULL,
	FECHA_END TEXT not NULL
	);

create table CONTACTO(
	ID VARCHAR(10) not NULL UNIQUE PRIMARY KEY,
	ID_SOPORTE VARCHAR(10) not NULL,
	NAVEGADOR TEXT not NULL,
	IP VARCHAR(100) not NULL,
	NOMBRE VARCHAR(100) not NULL,
	EMAIL VARCHAR(100) not NULL,
	MENSAJE TEXT not NULL,
	TELEFONO VARCHAR(100) not NULL,
	FECHA TEXT not NULL
	);

create table BRUTE(
	ID VARCHAR(10) not NULL UNIQUE PRIMARY KEY, 
	IP VARCHAR(20) not NULL, 
	IP_PROXY VARCHAR(20) not NULL, 
	FECHA VARCHAR(100) not NULL
	);

/* IMPORTANT: The database 'USUARIOS'  is the "name" of your database where you have your users (clients, wherever...). */

create table USUARIOS (
			ID VARCHAR(10) UNIQUE PRIMARY KEY, 
			NOMBRE VARCHAR(100),
			NICK VARCHAR(100), 
			PASSWORD TEXT, 
			EMAIL TEXT not NULL, 
			TIPO_USR VARCHAR(30), 
			FECHA_REGISTRO TEXT not NULL,  
			AVATAR VARCHAR(100) not NULL,  
			FECHA_NACIMIENTO TEXT not NULL,  
			ONLINE VARCHAR(1) not NULL 
);

